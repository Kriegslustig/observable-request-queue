{
  "name": "observable-request-queue",
  "version": "0.1.0",
  "description": "HTTP Request Queue Optmized for Web Workers",
  "main": "index.js",
  "repository": "git@github.com:Kriegslustig/observable-request-queue.git",
  "author": "Luca Nils Schmid <luca.schmid@vimcar.com>",
  "license": "MIT",
  "scripts": {
    "test": "ava",
    "test:watch": "ava --watch",
    "build": "babel src -d . --ignore **/__tests__/*.js",
    "build:watch": "chokidar src -c 'npm run build'",
    "prepublish": "npm run build",
    "flow": "flow",
    "flow:watch": "chokidar src -c 'flow'",
    "flow-typed": "flow-typed"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.0.0",
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.24.1",
    "chokidar-cli": "^1.2.0",
    "dom-event-target": "^1.0.0",
    "flow-bin": "^0.45.0",
    "flow-typed": "^2.1.2"
  },
  "ava": {
    "files": [
      "src/**/__tests__/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "rxjs": "^5.3.1",
    "uuid": "^3.0.1"
  }
}
